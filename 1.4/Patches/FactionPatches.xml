			<VREA_AndroidAwakened>0.02</VREA_AndroidAwakened>
		</value>
	</Operation>
	
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Royalty</li>
		</mods>
		<match Class="PatchOperationAdd">
			<xpath>Defs/FactionDef[defName="Empire"]/xenotypeSet/xenotypeChances</xpath>
			<value>
				<VREA_AndroidAwakened>0.02</VREA_AndroidAwakened>
			</value>
		</match>
